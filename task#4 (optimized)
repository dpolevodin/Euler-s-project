from datetime import datetime


a =(datetime.now())

def find_palindrom(min_range, max_range):
    result = []
    for i in range(min_range,max_range):
        for j in range(min_range,max_range):
            int_num = i * j
            num_str = str(int_num)
            if num_str == num_str[::-1]:
                result.append(int_num)
    return print(max(result)) 

find_palindrom(100, 1000)

b =(datetime.now())
print(b-a)
